  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch lnm",
      "program": "${workspaceFolder}/link-node-modules.js",
      "args": [
        "t",
        "../koa2-jsx"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Zoroaster Dev",
      "program": "${workspaceFolder}/node_modules/.bin/zoroaster",
      "env": {
        "ZOROASTER_TIMEOUT": "9999999"
      },
      "console": "integratedTerminal",
      "args": [
        "test/spec/generator/index.js",
        "--babel"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/example/index.js",
      "env": {
        "BABEL_ENV": "test"
      },
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    }
  ]
}
