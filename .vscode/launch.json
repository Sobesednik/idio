    {
      "type": "node",
      "request": "launch",
      "name": "Launch Zoroaster Dev",
      "program": "${workspaceFolder}/../../packages/zoroaster/bin/zoroaster.js",
      "env": {
        "ZOROASTER_TIMEOUT": "9999999"
      },
      "console": "integratedTerminal",
      "args": [
        "test/spec/generator/index.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/bin/idio"
